<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="QingXiao.mappers.UserRoleMapper">



    <insert id="insertUserRole" parameterType="java.util.HashMap">
        insert into user_role_table (user_role_id,role_id,user_id,create_time,privileges,status)
        values (#{userRoleID},#{roleID},#{userID},#{createTime},#{privileges},#{status})
    </insert>




</mapper>
